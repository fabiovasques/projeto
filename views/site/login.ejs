<% include ./header.ejs %>
<section id="principal">
  <div class="panel panel-default">
    <div class="panel-heading">Acesso Restrito</div>

    
  
  

      <div class="panel-body">
      <form action="/login/entrar" id="formLogin" data-toggle="validator" role="form" method="POST">
        <div class="form-group">
          <label for="inputEmail" class="control-label">E-mail</label>
          <input id="inputEmail" class="form-control" type="email" name="email" value=""  data-error="Por favor, informe um e-mail correto." required />
          <div class="help-block with-errors"></div>
        </div>
        <div class="form-group">
          <label for="textSenha" class="control-label">Senha</label>
          <input id="inputSenha" class="form-control" type="password" name="senha" value=""  data-error="Por favor, a sua senha." required />
          <div class="help-block with-errors"></div>
        </div>

        <div class="form-group">
          <button type="submit" class="btn btn-primary">Entrar</button>
        </div>
      </form>
    </div>
  </div>
</section>
<% include ./footer.ejs %>
